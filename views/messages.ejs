<%  if (success_message && success_message.length > 0) { %>
    <div class="alert just-go alert-success alert-dismissible fade show" role="alert">
        <%- success_message %> 
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
<% } %>

<%  if (error_message && error_message.length > 0) { %>
    <div class="alert just-go alert-error alert-dismissible fade show" role="alert">
        <%- error_message %> 
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
<% } %>